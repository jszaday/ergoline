package ergoline;

@system(alias = "std::string") class string {
    @system def toString(self: string): string;
    @system def plus(self: string, other: string): string;

    @system(alias = "std::stoi", static = true)
    def toInt(self: string): int;

    @system def tryParse<A>(self: string): option<A>;
}
