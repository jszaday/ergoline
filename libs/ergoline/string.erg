package ergoline;

@system(alias="std::string") struct string {
    @system(operator) def +(other: string): string;
    @system(operator) def ==(other: string): string;

    @system def toString(): string;

    @system(static, alias="std::stoi")
    def toInt(): int;

    @system def tryParse<A>(): option<A>;
}
