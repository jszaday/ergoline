package ergoline;

@system(alias = "std::string") struct string {
    @system(operator=true) def +(other: string): string;
    @system(operator=true) def ==(other: string): string;

    @system def toString(): string;

    @system(alias = "std::stoi", static = true)
    def toInt(): int;

    @system def tryParse<A>(): option<A>;
}
