package ergoline;

@system(alias = "std::vector", pointer=true)
class array<A> {
    @system def array();
    @system def array(n: int);

    @system(alias="[]")
    def get(idx: int): A;
    @system(alias="[]")
    def set(idx: int, value: A): unit;
    @system def size(): int;

    @system def getOrNone(idx: int): option<A>;
}
