package ergoline;

@system class int {
    @system def plus(other: int): int;
    @system def minus(other: int): int;
    @system def times(other: int): int;
    @system def rem(other: int): int;
    @system def equals(other: int): bool;
    @system def compareTo(other: int): int;

    @system(alias = "std::to_string", static = true)
    def toString(): string;
}
