package ergoline;

@system(alias="ergoline::hash_map")
class map<K, V> {
    @system def map();

    @system(static, alias="ergoline::map_contains")
    def contains(key: K): bool;

    @system(static, alias="ergoline::map_remove")
    @system def remove(key: K): bool;

    @system(alias="[]")
    def get(key: K): V;

    @system(alias="[]")
    def set(key: K, value: V);

    @system def size(): int;
}
