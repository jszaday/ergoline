package ergoline;

@system(alias="ergoline::hash_map", pointer=true)
class map<K, V> {
    @system def map();

    @system(alias="ergoline::map_contains", static=true)
    def contains(key: K): bool;

    @system(alias="ergoline::map_remove", static=true)
    @system def remove(key: K): bool;

    @system(alias="[]")
    def get(key: K): V;

    @system(alias="[]")
    def set(key: K, value: V);

    @system def size(): int;
}
