package ergoline;

@system(alias="std::deque",fromHeader="deque")
class queue<A> {
    @system(alias="front")
    def head(): A;
    @system(alias="back")
    def last(): A;
    @system(alias="empty")
    def isEmpty(): bool;
    @system(alias="empty",invert=true)
    def nonEmpty(): bool;
    @system(alias="push_back")
    def push(a: A);
    @system(alias="pop_front")
    def pop();
    @system def size(): int;
    @system def apply(): queue<A>;
}
