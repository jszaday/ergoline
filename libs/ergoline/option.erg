package ergoline;

@system class option<A> {
    @system def apply(t : A): option<A>;
    @system def apply(): option<A>;
    @system def isEmpty(): bool;
    @system def nonEmpty(): bool;
    @system def get(): A;
    @system def getOrElse(t: A): A;
    @system def flatMap<B>(f: (A => option<B>)): option<B>;
}
